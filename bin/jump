#!/bin/bash

if [ "$#" -lt 1 ] || [ -z "$*"]; then
	echo "No location given"
	$(exit 1)
elif [ grep -q '[.*]' <<< $* ]; then
	echo "Illegal charachters in jump command: $*"
	$(exit 2)	
elif ! [ -f "$ROOMS/$*/description" ]; then
	echo "Room not found"
	$(exit 3)
else
	cd "$ROOMS/$*"
	location=$($BIN/location)
	save user location
fi 2>/dev/null

